    User avivz

Host pond 
    HostName pond.cs.huji.ac.il
    User avivz

Host pond_jump
    HostName pond.cs.huji.ac.il
    User avivz
    
Host sed
    HostName sed.cs.huji.ac.il
    User avivz

Host rory
    HostName rory.cs.huji.ac.il
    User avivz


Host rory_jump
    HostName rory.cs.huji.ac.il
    User avivz

Host cryptocur_jump
    HostName rory.cs.huji.ac.il
    User cryptocur

Host cryptocur
    HostName rory.cs.huji.ac.il
    User cryptocur

Host *_jump
    ProxyJump bava-jump-box

Host *
    Compression yes
    #ForwardX11 yes